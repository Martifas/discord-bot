{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "**The bot will need to:**",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "tsx src",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "tsx watch src",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "migrate:latest": "tsx src/database/migrate/bin",
    "insert": "tsx src/database/utils/createDatabaseEntry",
    "gen:types": "kysely-codegen --camel-case --dialect sqlite --out-file src/database/types.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.8.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^2.1.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "kysely-codegen": "^0.17.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tsx": "^4.19.1",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "@giphy/js-fetch-api": "^5.6.0",
    "axios": "^1.7.7",
    "better-sqlite3": "^11.5.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "http-status-codes": "^2.3.0",
    "kysely": "^0.27.4",
    "lodash": "^4.17.21",
    "npm-check-updates": "^17.1.6",
    "package": "^1.0.1",
    "zod": "^3.23.8"
  }
}
